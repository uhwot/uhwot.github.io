{
  "devDependencies": {
    "esbuild": "^0.14.21",
    "gh-pages": "^3.2.3"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "three": "^0.137.5"
  },
  "scripts": {
    "build": "esbuild src/main.js --bundle --sourcemap --outfile=public/build/bundle.js",
    "build-prod": "esbuild src/main.js --bundle --minify --outfile=public/build/bundle.js --target=chrome98,firefox97",
    "devserver": "esbuild src/main.js --bundle --sourcemap --outfile=public/build/bundle.js --servedir=public"
  }
}
